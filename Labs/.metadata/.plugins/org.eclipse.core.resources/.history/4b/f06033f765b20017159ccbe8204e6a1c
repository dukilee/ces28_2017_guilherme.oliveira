package ex1;

public class HeartPhoto extends Photo{

	@Override
	public String toPrint() {
		return
				"     __        __                 \n"+
				"    /  \\      /  \\                \n"+
				"   /    \\    /    \\               \n"+
				"  /      \\  /      \\              \n"+
				" /        \\/        \\             \n"+
				" \\                  /             \n"+
				"  \\                /              \n"+
				"   \\              /               \n"+
				"    \\            /                \n"+
				"     \\          /                 \n"+
				"      \\        /                  \n"+
				"       \\      /                   \n"+
				"        \\    /                    \n"+
				"         \\  /                     \n"+
				"          \\/                      \n"+
				"                                  \n";
	}
	
}
